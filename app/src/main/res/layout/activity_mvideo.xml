<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sp"
    tools:context=".mVideo">

    <TextView
        android:id="@+id/textView39"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView40"
        android:layout_centerHorizontal="true"
        android:text="Farishta" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView39"
        android:text="Farishta" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageButton4"
        android:text="Farishta" />



    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="135dp"
        android:layout_height="81dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/v2" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="135dp"
        android:layout_height="81dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/v3" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="135dp"
        android:layout_height="81dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/v4" />

    <WebView
        android:id="@+id/mvideo"
        android:layout_width="360dp"
        android:layout_height="345dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="77dp"
        android:layout_marginStart="11dp"
        android:background="@drawable/sp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="128dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageButton2"
        android:text="Farishta" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="121dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton"
        android:text="Let's|2015" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="109dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton2"
        android:text="Be a Hero" />
</RelativeLayout>